package Study;



import java.util.*;
public class Pratice {

	public static void main(String[] args)  {
	
		int a=0,b = 5, c=3;
		
		a=b+c;
		System.out.printf("%d+%d =%d \n",b,c,a);
		
		a= b-c;
		System.out.printf("%d-%d=%d\n",b,c,a);
	
		a= b*c;
		System.out.printf("%d*%d=%d \n",b,c,a);
	
		a=b/c;
		System.out.printf("%d/%d=%d\n",b,c,a);

		a=b%c;
		System.out.printf("%d%%%d=%d\n",b,c,a);
	
	}
}
